                        <Font name="System Bold" size="12.0" />
                     </font>
                     <padding>
                        <Insets left="10.0" right="10.0" />
                     </padding>
                  </Button>
                  <Button id="youtubeButton" mnemonicParsing="false" prefHeight="35.0" styleClass="icon-youtube" text="YouTube обзор">
                     <padding>
                        <Insets left="10.0" right="10.0" />
                     </padding>
                  </Button>
                  <HBox alignment="TOP_RIGHT" prefWidth="20.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Button id="cancelButton" maxHeight="1.7976931348623157E308" mnemonicParsing="false" text="Закрыть">
                           <padding>
                              <Insets left="10.0" right="10.0" />
                           </padding>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
